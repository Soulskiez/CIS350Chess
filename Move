package project;

public class Move {

	public int fromRow;
	
	public int fromColumn;
	
	public int toRow;
	
	public int toColumn;
	
	public Move(){
		

	}
	
	public Move(int fromRow, int fromColumn, int toRow, int toColumn){
		this.fromRow = fromRow;
		this.fromColumn = fromColumn; 
		this.toRow = toRow;
		
	}
	
}
